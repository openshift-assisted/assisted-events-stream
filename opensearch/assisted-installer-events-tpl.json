{
  "index_patterns":["assisted-installer-events-v1-*"],
  "settings" : {
    "index" : {
      "number_of_shards" : "1",
      "number_of_replicas" : "1",
      "mapping" : {
        "total_fields" : {
          "limit" : "100000"
        }
      },
      "codec": "best_compression"
    }
  },
  "mappings": {
    "dynamic_templates":[
      {
        "default_keyword": {
          "match_mapping_type": "string",
          "mapping": {
            "type": "keyword",
            "ignore_above": 2048
          }
        }
      }
    ],
    "properties": {
      "cluster": {
        "dynamic": true,
        "properties": {
          "connectivity_majority_groups": {
            "type": "object",
            "enabled": false
          },
          "feature_usage": {
            "dynamic": true,
            "properties": {}
          },
          "hosts": {
            "dynamic": true,
            "properties": {}
          }
        }
      },
      "event_time": {
        "type": "date"
      },
      "message": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 2048
          }
        }
      },
      "versions": {
        "dynamic": true,
        "properties": {}
      }
    }
  },
  "aliases" : {
    "assisted-service-events" : { },
    "read-assisted-service-events" : { }
  }
}
